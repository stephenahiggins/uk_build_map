# Note: Any values need to also be added to envValues.ts
# LLM Provider (default: OpenAI)
OPENAI_API_KEY=
GEMINI_API_KEY=
# DATABASE_URL=postgres://user:pass@localhost:5432/db
DATABASE_URL="file:./storage/data.db"
MODEL=gpt-5-mini
# Optional provider-specific overrides
OPENAI_MODEL=gpt-4o-mini
GEMINI_MODEL=gemini-2.5-flash
PROVIDER=openai

# Testing and Development
MOCK_PROJECT_STATUS=false
# MOCK_EVIDENCE_ANALYSIS=false
MOCK_INFRASTRUCTURE_SEARCH=false

# Backend DB URL used during migrate (must be a fully expanded literal URL)
MIGRATE_DATABASE_URL="mysql://root:123456@db:3306/node_boilerplate"
