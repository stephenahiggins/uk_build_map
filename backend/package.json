{
  "name": "boilerplate-ts",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "seed:projects": "ts-node scripts/seedProjectsFromFile.ts projects.seed.json",
    "build": "tsc --project tsconfig.build.json && tsc-alias",
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "test": "jest -c jest.config.js --env=node",
    "test:watch": "jest -c jest.config.js -i --watchAll",
    "test:cov": "jest -c jest.config.js --coverage --env=node",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "lint": "npx eslint src --no-cache --fix",
    "format": "npx prettier --write src/**/*.{js,ts,json}",
    "loglevel:change": "runtime-node-refresh",
    "seed": "ts-node prisma/seed.ts",
    "recompute:evaluations": "ts-node scripts/recomputeProjectEvaluations.ts"
  },
  "prisma": {
    "seed": "npx ts-node --project tsnode.json prisma/seed.ts"
  },
  "_moduleAliases": {
    "@/src": "src/"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "npm run lint"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google/genai": "^1.20.0",
    "@prisma/client": "^5.18.0",
    "@types/cookie-parser": "^1.4.7",
    "@types/morgan": "^1.9.3",
    "@types/swagger-ui-express": "^4.1.6",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csv-parse": "^5.6.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-http-context": "^1.2.4",
    "google-auth-library": "^9.14.0",
    "helmet": "^7.1.0",
    "http-status": "^1.7.4",
    "husky": "^8.0.3",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.15",
    "openai": "^5.21.0",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.22",
    "@types/jest": "^29.5.12",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.15.3",
    "@types/node-fetch": "^2.6.12",
    "@types/nodemailer": "^6.4.15",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.2.0",
    "@typescript-eslint/parser": "^8.2.0",
    "eslint": "^9.9.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.9.0",
    "jest": "^29.7.0",
    "kill-port": "^2.0.1",
    "lint-staged": "^15.2.9",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "prisma": "^5.18.0",
    "runtime-node-refresh": "^1.0.7",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.2.0",
    "uuid": "^10.0.0",
    "winston": "^3.14.2"
  },
